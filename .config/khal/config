[calendars]

[[posteo]]
path = ~/.local/share/vdirsyncer/posteo/default
type = calendar
color = yellow

[locale]
timeformat = %I:%M %p
dateformat = %m/%d/%y
longdateformat = %B %d, %Y
datetimeformat = %m/%d/%y at %I:%M %p
longdatetimeformat = %B %d, %Y at %I:%M %p
unicode_symbols = False

[highlight_days]
color = yellow

[view]
frame = top
